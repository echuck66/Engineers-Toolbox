<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="EngineersToolbox.Views.Calculators.FuelEconomyCalculator"
             Title="FuelEconomyCalculator">
    <VerticalStackLayout>
        <Image Margin="10">
            <Image.Source>
                <FontImageSource
                        FontFamily="FontAwesomeSolid"
                        Glyph="{StaticResource fuel}"
                        Color="{StaticResource Primary}"
                        Size="43"/>
            </Image.Source>
        </Image>

        <Label Margin="10" VerticalOptions="End" HorizontalOptions="Start">Copyright 2023, CrawfordNET Solutions, LLC</Label>

    </VerticalStackLayout>
</ContentPage>